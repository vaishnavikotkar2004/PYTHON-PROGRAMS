Q.1
def f(a,b):
    q=a//b
    r=a%b
    print("quotient=",q)
    print("remainder=",r)
Q.2
from math import*
def circle(r):
    a=pi*r**2
    c=2*pi*r
    print("area of circle=",a)
    print("area of circumference=",c)
Q.3
from cmath import*
def f(a,b,c):
    r=-b/(2*a)
    i=sqrt(b*b-4*a*c)/(2*a)
    print("roots=",r+i,',',r-i)
